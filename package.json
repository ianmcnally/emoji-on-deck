{
  "name": "emoji-on-deck",
  "version": "0.1.10",
  "description": "A fast emoji search",
  "main": "app/index.js",
  "scripts": {
    "ci": "npm run lint && npm run test:travisci",
    "compile": "npm run compile:clean && npm run compile:indexhtml && npm run compile:assets",
    "compile:clean": "rm -f {index.html,main.js,main.css}",
    "compile:indexhtml": "cp src/index.html .",
    "compile:assets": "NODE_ENV=production webpack",
    "deploy": "npm run deploy:build && npm run deploy:commit && npm run deploy:push && npm run deploy:cleanup",
    "deploy:build": "git branch -D gh-pages && git checkout -b gh-pages && npm run compile && git add -f index.html main.js main.css",
    "deploy:commit": "git commit -a -m $npm_package_version",
    "deploy:push": "git push -f origin gh-pages",
    "deploy:cleanup": "git checkout - && npm run compile:clean",
    "lint": "eslint --ext .jsx,.js dev-server src && jscs -x dev-server src",
    "start": "env NODE_ENV=development node dev-server",
    "test": "env NODE_ENV=test karma start karma.conf.js",
    "test:travisci": "env PHANTOMJS_BIN=/usr/local/bin/phantomjs npm test",
    "watch": "npm run test -- --single-run=false"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "react": "^15.1.0",
    "react-dom": "^15.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^4.0.2",
    "css-loader": "^0.28.4",
    "es5-shim": "^4.4.1",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "imports-loader": "^0.7.1",
    "jscs": "^3.0.7",
    "jsx-chai": "^4.0.0",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lodash.assign": "^4.0.6",
    "mocha": "^3.4.2",
    "postcss-loader": "^2.0.6",
    "postcss-normalize": "^3.0.0",
    "react-addons-test-utils": "^15.6.0",
    "react-shallow-testutils": "^3.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "render-shallow": "^1.1.0",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "author": "Ian McNally <ianmcnally@gmail.com>",
  "license": "ISC"
}
